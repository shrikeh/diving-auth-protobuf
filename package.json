{
  "name": "@shrikeh/diving-auth-protobuf",
  "version": "0.1.0",
  "type": "module",
  "files": [
    "src/"
  ],
  "devDependencies": {
    "@grpc/grpc-js": "1.4.1",
    "@types/node": "^16.11.0",
    "@types/protobufjs": "^6.0.0",
    "dotenv": "^10.0.0",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "grpc-tools": "^1.11.2",
    "protobufjs": "^6.11.2"
  },
  "scripts": {
    "protobuf:generate": "yarn protobuf:generate-js && yarn protobuf:generate-ts",
    "protobuf:generate-js": "pbjs -t static-module -w es6 protobuf/auth.proto > src/Protobuf/auth.js",
    "protobuf:generate-ts": "pbts -o src/Protobuf/auth.d.ts src/Protobuf/auth.js"
  }
}
